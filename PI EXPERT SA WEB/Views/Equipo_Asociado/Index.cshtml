@model PI_EXPERT_SA_WEB.Models.ModeloIntermedioRolEmpleadoHabilidades

@{
    ViewBag.Title = "EquipoAsociado";
}

<h2>Equipo</h2>

<table class="table">
    <tr>
        <th></th>
        <th>Cedula</th>
        <th>Nombre</th>
        <th>Apellido 1</th>
        <th>Apellido 2</th>
    </tr>

    @for(int i = 0; i < Model.empleados.Count; i++)
    {
<tr>
    <td><input type="checkbox" /></td>
    <td> @Html.DisplayFor(modelItem => Model.empleados.ElementAt(i).cedulaPK</td>
    <td> @Html.DisplayFor(modelItem => Model.empleados.ElementAt(i).nombre</td>
    <td> @Html.DisplayFor(modelItem => Model.empleados.ElementAt(i).apellido1</td>
    <td> @Html.DisplayFor(modelItem => Model.empleados.ElementAt(i).apellido2</td>

</tr>
    }
</table>
