@model List<PI_EXPERT_SA_WEB.Models.EMPLEADO>

@{
    ViewBag.Title = "DesarrolladoresDisponibles";
}


<table>
    <tr>
        <th>
            Nombre Empleado
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3" style="color:red">
                No hay resultados
            </td>
        </tr>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(ModelItem => item.nombre) @Html.DisplayFor(ModelItem => item.apellido1) @Html.DisplayFor(ModelItem => item.apellido2)
                </td>
            </tr>
        }
    }
</table>