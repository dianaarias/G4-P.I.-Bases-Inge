@using PI_EXPERT_SA_WEB.Models
@model List<Group<string, CONSULTAS>>

@*Esta vista muestra la cantidad de desarrolladores de una habilidad en especifica y el promedio
    de antiguedad entre los desarrolladores*@

<h2>Cantidad de desarrolladores por habilidad y promedio de antigüedad</h2>

<table class="table table-bordered table-striped" id="tabla">
    <tr>
        <th>
            Habilidad
        </th>
        <th>
            Cantidad desarrolladores
        </th>
        <th>
            Promedio antiguedad (años)
        </th>
    </tr>

    @foreach (var item in Model)
    {

        <tr>
            <td>
                @item.Key
            </td>
            <td>
                @item.suma
            </td>
            <td>
                @item.avg
            </td>

        </tr>
    }
</table>


