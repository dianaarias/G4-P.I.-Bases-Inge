@*@model IEnumerable<PI_EXPERT_SA_WEB.Models.Group<string, CONSULTA  >>*@

@using PI_EXPERT_SA_WEB.Models
    @model List<Group<string, CONSULTAS>>


@*Lista de agrupación por complejidad en requerimiento*@

<h2>Comparación de duraciones para requerimientos del mismo nivel de complejidad</h2>

<table class="table table-bordered table-striped" id="tabla">
    <tr>
        <th>
            Complejidad
        </th>
        <th>
            Total de Requerimientos
        </th>
        <th>
            Valor mínimo
        </th>
        <th>
            Valor máximo
        </th>
        <th>
            Promedio Duración Real
        </th>
    </tr>

    @foreach (var group in Model)
    {

    <tr>
        <td>
            @group.Key
        </td>

        <td>
            @group.suma
        </td>
        <td>
            @group.minimo
        </td>
        <td>
            @group.maximo
        </td>
        <td>
            @group.promedio
        </td>
    </tr>
    }
</table>

