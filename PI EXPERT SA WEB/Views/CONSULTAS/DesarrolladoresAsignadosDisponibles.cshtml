@model  List<IQueryable>

    <div>
        <table class="table table-bordered table-striped">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.apellido1)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.correo)
                </th>
                <th>
                    Ver más información
                </th>
            </tr>
            @if (Model.Count() == 0)
            {
                <tr>
                    <td colspan="3" style="color:red">
                        No hay resultados
                    </td>
                </tr>
            }
            else
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.apellido1)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.correo)
                        </td>
                        <td>
                            @*Aqui se muestra el boton para buscar detalles*@
                            <a href="@Url.Action("Details", "CLIENTE", new { id = item.cedulaPK })" style="float:unset">
                                <img src="~/images/more.svg" width="30" height="30" />
                            </a>
                        </td>
                    </tr>
                }
            }

        </table>
    </div>
