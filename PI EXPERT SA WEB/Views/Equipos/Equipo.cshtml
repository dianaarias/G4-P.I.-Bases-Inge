
@{
    ViewBag.Title = "Equipo";
}

<h2>Equipo</h2>

<table class="table">
    <tr>
        <th>Nombre</th>
        <th>Primer Apellido</th>
        <th>Segundo Apellido</th>
        <th>Correo</th>
    </tr>

    @for (int iterator = 0; iterator < Model.listaEmpleados.Count; iterator++)
    {
<tr>
    <td>@Html.DisplayFor(modelItem => Model.listaEmpleados.ElementAt(1).nombre)</td>
    <td>@Html.DisplayFor(modelItem => Model.listaEmpleados.ElementAt(1).apellido1)</td>
    <td>@Html.DisplayFor(modelItem => Model.listaEmpleados.ElementAt(1).apellido2)</td>
    <td>@Html.DisplayFor(modelItem => Model.listaEmpleados.ElementAt(1).correo)</td>
</tr>
    }
</table>

