@model IEnumerable<PI_EXPERT_SA_WEB.Models.MODULO>

@*@model PI_EXPERT_SA_WEB.Models.MODULO*@

@{
    ViewBag.Title = "ListaParcialModulos";
}



    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fechaInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PROYECTO.cedulaClienteFK)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PROYECTO.cedulaClienteFK)
            </td>
            <td>
                <div class="col text-center">
                    @Html.ActionLink("Más información", "Details", new { idModuloPK = item.idModuloPK, idProyectoPK = item.idProyectoPK }, new { @class = "btn btn-outline-secondary btn-sm text-center" })
                </div>
            </td>
        </tr>
        }

    </table>


